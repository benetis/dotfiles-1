[user]
	name = MSHELL_REALNAME
	email = MSHELL_EMAIL
[alias]
	lg  = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ar) %C(bold blue)<%an>%Creset' --abbrev-commit --date-order
	lga = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ar) %C(bold blue)<%an>%Creset' --abbrev-commit --date-order --all
	s = status
	c = diff -M --cached
	d = diff -M
	a = add
	sync = !git-sync
	pc = !git-pc
[core]
	safecrlf = false
[color]
	ui = true
[merge]
	conflictstyle = diff3
	defaultToUpstream = true
[push]
	default = nothing
