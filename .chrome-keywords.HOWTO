Export:
 cp ".config/google-chrome/Default/Web Data" /tmp/kupa
 sqlite3 /tmp/kupa "update keywords set usage_count = 0"
 sqlite3 /tmp/kupa ".dump keywords" > .chrome-keywords.sql
 rm /tmp/kupa

Import:
 $ sqlite3 ".config/google-chrome/Default/Web Data"
 > drop table keywords;
 > .read .chrome-keywords.sql
